           </div>
           <% if flash[:notice] %>
             <div class="notice"><%= flash[:notice] %></div>
           <% end %>
           <% @campaigns.each do |c| %>
             <div class="w-row">
                 <div class="w-col w-col-4">
                     <div class="card-project card u-radius">
                         <a href="/campaigns/<%= c.id %>" class="card-project-thumb" style="background-image: url(&quot;<%= c.image %>&quot;); display: block;"></a>
                         <div>
                             <div class="card-project-description alt">
                                 <div class="fontweight-semibold u-text-center-small-only lineheight-tight u-marginbottom-10 fontsize-base"><a href="/campaigns/<%= c.id %>" class="link-hidden"><%= c.name %></a></div>
                                 <div class="w-hidden-small w-hidden-tiny fontsize-smallest fontcolor-secondary u-marginbottom-20"><%= c.creator.username %></div>
                                 <div class="w-hidden-small w-hidden-tiny fontcolor-secondary fontsize-smaller"><a href="/campaigns/<%= c.id %>" class="link-hidden"><%= c.description %></a></div>
                             </div>
                             <div class="card-project-meter aon online incomplete">
                                 <div class="meter">
                                     <div class="meter-fill" style="width: <%= c.percent_complete %>;"></div>
                                 </div>
                             </div>
                             <div class="card-project-stats">
                                 <div class="w-row">
                                     <div class="w-col w-col-4 w-col-small-4 w-col-tiny-4">
                                         <div class="fontsize-base fontweight-semibold"><%= c.percent_complete %></div>
                                     </div>
                                     <div class="w-col w-col-4 w-col-small-4 w-col-tiny-4 u-text-center-small-only">
                                         <div class="fontsize-smaller fontweight-semibold">$<%= c.raised %></div>
                                         <div class="fontsize-smallest lineheight-tightest">Raised</div>
                                     </div>
                                     <div class="w-col w-col-4 w-col-small-4 w-col-tiny-4 u-text-right">
                                         <div class="fontsize-smaller fontweight-semibold"> <%= c.days_left %> days</div>
                                         <div class="fontsize-smallest lineheight-tightest">Remaining</div>
                                     </div>
                                 </div>
                             </div>
                         </div>
                     </div>
                 </div>
              <% end %>
